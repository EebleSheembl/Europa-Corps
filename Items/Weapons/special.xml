				</Explosion>
			</StatusEffect>

			<StatusEffect type="Always" target="This" interval="0.1" comparison="And">
				<Conditional IsOn="eq True"/>

				<ParticleEmitter particle="chemmist" AngleMax="360" DistanceMax="50" VelocityMin="100" VelocityMax="150" ScaleMultiplier="2,2" ParticleAmount="2" ColorMultiplier="255,69,0,155" />
