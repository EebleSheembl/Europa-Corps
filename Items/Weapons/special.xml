<Items>
	<!--Nerve Agent-->
	<Item name="" identifier="fenitergasgrenade1" category="Weapon" maxstacksize="8" cargocontaineridentifier="chemicalcrate" tags="smallitem,weapon,explosive" Scale="0.5" impactsoundtag="" health="15">
		
		<Sprite texture="sprites/chemicalwarfaremain.png" sourcerect="20,11,27,53" depth="0.55" origin="0.5,0.5" />
		
		<Body width="26" height="54" density="30" />
		
		<ItemComponent characterusable="false">
			<StatusEffect type="OnUse" target="This" disabledeltatime="false" tags="armed" />
		</ItemComponent>
		
		<Throwable 
			slots="Any,RightHand,LeftHand" 
			holdpos="0,0" handle1="0,0" throwforce="3.5" 
			msg="ItemMsgPickUpSelect">
		
			<StatusEffect type="OnSecondaryUse" target="This" delay="2" disabledeltatime="false" tags="armed" />

			<StatusEffect type="Always" target="This" interval="0.1" comparison="And">
				<Conditional condition="gt 0" />
				<Conditional hastag="eq armed" />
				<ParticleEmitter particle="chemmist" AngleMax="360" DistanceMax="50" VelocityMin="100" VelocityMax="150" ScaleMultiplier="2,2" ParticleAmount="2" ColorMultiplier="255,69,0,155" />
			</StatusEffect>

			<StatusEffect type="Always" target="This" disabledeltatime="false" comparison="And" interval="0.5" offset="0,100" condition="-0.5">
				<Conditional condition="gt 0" />
				<Conditional hastag="eq armed" />

				<Explosion showeffects="false" range="500">
					<Affliction identifier="feniternerveagent" strength="1" />
					<Affliction identifier="feniternerveagentcon" strength="2" />
				</Explosion>
			</StatusEffect>

		</Throwable>
	
	</Item>


